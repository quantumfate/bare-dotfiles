Host *
	ServerAliveInterval 360
	ControlMaster auto
        ControlPath ~/.ssh/controlmasters/%r@%h:%p
	ControlPersist yes
	IdentitiesOnly yes
	LogLevel INFO

Host quantumhome
	HostName 192.168.1.23
	User sao
	Port 6969
	IdentityFile ~/.ssh/quantumhome

Host pihole
	HostName 192.168.2.12
	User pi
	Port 6969
	IdentityFile ~/.ssh/pihole

Host github.com
	User git
	IdentityFIle ~/.ssh/github

Host git.haw-hamburg.de
	User git
	IdentityFile ~/.ssh/gitlab
